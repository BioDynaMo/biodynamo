<lcgdict>
  <selection>
    <class pattern="bdm::*" />
    <class pattern="std::vector<bdm::*>" />
  </selection>
  <exclusion>
    <class pattern="cl::*" />
    <class name="cl::CommandQueue" />
    <class name="cl::Context" />
    <class name="cl::Program" />
    <class pattern="*_TC2*" />
    <class pattern="std::array<*>" />
    <class name="chrono::system_clock"/>
    <class name="chrono::steady_clock"/>
    <class pattern="*_Test" />
    <class name="__gnu_cxx::__normal_iterator<double*,vector<double> >"/>
    <class name="__gnu_cxx::__normal_iterator<const double*,vector<double> >"/>
    <class name="std::function<void(bdm::SimObject*)>"/>
    <!-- list of classes with dictionary in libbiodynamo
         -> all selected classes from libbiodynamo-selection.xml -->
    <class name="bdm::experimental::neuroscience::NeuronSoma" />
    <class name="bdm::experimental::neuroscience::NeuriteElement" />
    <class name="bdm::SoPointer<bdm::experimental::neuroscience::NeuronOrNeurite>" />
    <class name="bdm::SoPointer<bdm::experimental::neuroscience::NeuriteElement>" />
    <class name="bdm::experimental::neuroscience::Param" />
    <class name="bdm::DiffusionGrid" />
    <class name="bdm::Simulation" />
    <class name="bdm::ResourceManager" />
    <class name="bdm::SimObject" />
    <class name="bdm::SoHandle" />
    <class name="bdm::SoUid" />
    <class name="bdm::SoUidGenerator" />
    <class name="bdm::Cell" />
    <class name="std::unordered_map<uint64_t, bdm::DiffusionGrid*>" />
    <class name="std::unordered_map<uint64_t, bdm::SoHandle>" />
    <class name="bdm::RuntimeVariables"/>
    <class name="bdm::BaseBiologyModule"/>
    <class name="bdm::GrowDivide"/>
    <class name="bdm::IntegralTypeWrapper<size_t> "/>
    <class name="bdm::RegulateGenes" />
    <class name="bdm::Param" />
    <class name="bdm::ModuleParam" />
    <class name="unordered_map<unsigned long,bdm::ModuleParam*>" />
    <class name="bdm::Random" />
    <class name="bdm::MathArray<double, 3>" />
    <class name="bdm::MathArray<double, 4>" />
    <!-- list of biodynamo classes that should be ignored  -->
    <class name="bdm::Operation" />
    <class name="bdm::SoVisitor" />
    <class name="bdm::Grid" />
    <class name="bdm::Grid::NeighborMutexBuilder::MutexWrapper" />
    <class name="bdm::ParallelResizeVector<double>" />
    <class name="bdm::ParallelResizeVector<bdm::Grid::Box>" />
    <class name="bdm::ParallelResizeVector<std::pair<unsigned int, bdm::Grid::Box const*>>" />
  </exclusion>
</lcgdict>
