<lcgdict>
  <selection>
    <class pattern="bdm::*<*>"/>
    <class pattern="bdm::*Module"/>
    <class pattern="bdm::*Behaviour"/>
    <class name="bdm::simulation_object_util_test_internal::Neurite" />
    <class pattern="std::vector<bdm::*<*>>"/>
    <class pattern="std::tuple<bdm::*<*>"/>
  </selection>
  <exclusion>
    <class pattern="std::array<*>" />

    <class pattern="*bdm::SoaRef*"/>
    <class name="chrono::system_clock"/>
    <class pattern="*_Test" />
    <!-- genreflex generates wrong code:
         SimulationObjectVector<ResourceManager...> -->
    <class pattern="bdm::SimulationObjectVector*" />
    <!-- list of classes with dictionary in libbiodynamo
         -> all selected classes from libbiodynamo-selection.xml -->
    <class name="bdm::RuntimeVariables"/>
    <class name="bdm::NullBiologyModule"/>
    <class name="bdm::IntegralTypeWrapper<size_t> "/>
    <!-- list of template classes with custom streamers -->
    <class pattern="bdm::Variant*" />
  </exclusion>
</lcgdict>
