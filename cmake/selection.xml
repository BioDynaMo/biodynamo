<lcgdict>
  <selection>
    <class pattern="bdm::InlineVector*" noStreamer="true" />
    <class pattern="bdm::*" />
    <class pattern="std::vector<bdm::*>" />
  </selection>
  <exclusion>
    <class pattern="bdm::OpenCLState*"/>
    <class name="bdm::OperationImpl" />
    <class pattern="cl::*" />
    <class name="cl::CommandQueue" />
    <class name="cl::Context" />
    <class name="cl::Program" />
    <class pattern="*_TC2*" />
    <class pattern="std::array<*>" />
    <class name="chrono::system_clock"/>
    <class name="chrono::steady_clock"/>
    <class pattern="*_Test" />
    <class name="__gnu_cxx::__normal_iterator<double*,vector<double> >"/>
    <class name="__gnu_cxx::__normal_iterator<const double*,vector<double> >"/>
    <class name="std::function<void(bdm::Agent*)>"/>
    <!-- list of classes with dictionary in libbiodynamo
         -> all selected classes from libbiodynamo-selection.xml -->
    <class name="bdm::neuroscience::NeuronSoma" />
    <class name="bdm::neuroscience::NeuriteElement" />
    <class name="bdm::AgentPointer<bdm::neuroscience::NeuronOrNeurite>" />
    <class name="bdm::AgentPointer<bdm::neuroscience::NeuriteElement>" />
    <class name="bdm::neuroscience::Param" />
    <class name="bdm::EulerGrid" />
    <class name="bdm::StencilGrid" />
    <class name="bdm::RungaKuttaGrid" />
    <class name="bdm::DiffusionGrid" />
    <class name="bdm::Simulation" />
    <class name="bdm::ResourceManager" />
    <class name="bdm::Agent" />
    <class name="bdm::AgentHandle" />
    <class name="bdm::AgentUid" />
    <class name="bdm::AgentUidGenerator" />
    <class name="bdm::Cell" />
    <class name="std::unordered_map<uint64_t, bdm::DiffusionGrid*>" />
    <class name="std::unordered_map<uint64_t, bdm::AgentHandle>" />
    <class name="bdm::RuntimeVariables"/>
    <class name="bdm::Behavior"/>
    <class name="bdm::GrowthDivision"/>
    <class name="bdm::Chemotaxis"/>
    <class name="bdm::Secretion"/>
    <class name="bdm::IntegralTypeWrapper<size_t> "/>
    <class name="bdm::GeneRegulation" />
    <class name="bdm::Param" />
    <class name="bdm::ParamGroup" />
    <class name="unordered_map<unsigned long,bdm::ParamGroup*>" />
    <class name="bdm::Random" />
    <class name="bdm::MathArray<double, 3>" />
    <class name="bdm::MathArray<double, 4>" />
    <class name="bdm::InlineVector<bdm::Agent*, 3>" noStreamer="true" />
    <class name="bdm::InlineVector<bdm::Behavior*, 2>" noStreamer="true" />
    <class name="bdm::InlineVector<bdm::Behavior*, 3>" noStreamer="true" />
    <class name="bdm::VectorTypeWrapper<bdm::Behavior*>" />
    <!-- list of biodynamo classes that should be ignored  -->
    <class pattern="bdm::memory_manager_detail::*" />
    <class pattern="std::vector<bdm::memory_manager_detail::*>" />
    <class name="bdm::MemoryManager" />
    <class name="bdm::Operation" />
    <class name="bdm::SoVisitor" />
    <class name="bdm::Environment" />
    <class name="bdm::Environment::SimDimensionAndLargestAgentFunctor" />
    <class name="bdm::UniformGridEnvironment" />
    <class name="bdm::UniformGridEnvironment::GridNeighborMutexBuilder::MutexWrapper" />
    <class name="bdm::ParallelResizeVector<double>" />
    <class name="bdm::ParallelResizeVector<bdm::MathArray<double, 3ul>>" />
    <class name="bdm::ParallelResizeVector<bdm::Grid::Box>" />
    <class name="bdm::ParallelResizeVector<std::pair<unsigned int, bdm::Grid::Box const*>>" />
  </exclusion>
</lcgdict>
